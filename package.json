{
  "name": "@sample_test/lokijs",
  "version": "2.0.0-beta.5",
  "description": "Fast document oriented javascript in-memory database",
  "author": "Various authors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LokiJS-Forge/LokiJS2.git"
  },
  "bugs": {
    "url": "https://github.com/LokiJS-Forge/LokiJS2/issues"
  },
  "keywords": [
    "javascript",
    "document-oriented",
    "mmdb",
    "database",
    "json",
    "lokijs",
    "in-memory",
    "full-text-search"
  ],
  "scripts": {
    "lint": "eslint --fix './src/**/*.js' './bench/**/*.bench.js' './spec/**/*.spec.js' -c config/eslintrc.js || true",
    "test": "npm-run-all test:web test:node",
    "test:web": "cross-env NODE_ENV=test karma start config/karma.config.js --single-run || true",
    "test:node": "jasmine --config='config/jasmine.json' || true",
    "bench": "karma start config/karma-bench.config.js --single-run || true",
    "doc": "jsdoc -c config/jsdoc-conf.json",
    "preversion": "npm run lint && npm run test",
    "version": "./build.sh",
    "postversion": "git add dist/packages/ && git commit --amend --no-edit && git push && npm publish"
  },
  "devDependencies": {
    "add-matchers": "^0.5.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-register": "^6.22.0",
    "benchmark": "^2.1.3",
    "cross-env": "^4.0.0",
    "elasticsearch": "^13.2.0",
    "eslint": "^3.13.1",
    "eslint-loader": "^1.6.1",
    "jasmine": "^2.6.0",
    "jasmine-expect": "^3.6.0",
    "jsdoc": "^3.4.3",
    "karma": "^1.4.0",
    "karma-benchmark": "^0.7.1",
    "karma-benchmarkjs-reporter": "^1.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-matchers": "^3.2.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-webpack": "^2.0.1",
    "npm-run-all": "^4.0.1",
    "source-map": "^0.5.6",
    "uglify-es": "^3.0.25",
    "webpack": "^2.5.1"
  },
  "dependencies": {}
}
