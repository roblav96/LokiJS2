{
  "name": "LokiJS2",
  "version": "2.0.1",
  "description": "Fast document oriented javascript in-memory database",
  "homepage": "http://lokijs.org",
  "main": "lib/library.js",
  "scripts": {
    "build": "webpack",
    "dev": "webpack -p",
    "lint": "eslint ./src/**/*.js ./bench/**/*.bench.js ./spec/**/*.spec.js -c config/eslintrc.js || true",
    "lint:fix": "eslint --fix ./src/**/*.js ./bench/**/*.bench.js ./spec/**/*.spec.js -c config/eslintrc.js || true",
    "test": "npm-run-all test:web test:node",
    "test:web": "cross-env NODE_ENV=test karma start config/karma.config.js --single-run || true",
    "test:node": "jasmine || true",
    "bench": "karma start config/karma-bench.config.js --single-run || true",
    "doc": "jsdoc -c config/jsdoc-conf.json"
  },
  "devDependencies": {
    "add-matchers": "^0.5.0",
    "benchmark": "^2.1.3",
    "cross-env": "^4.0.0",
    "eslint": "^3.13.1",
    "eslint-loader": "^1.6.1",
    "jasmine": "^2.5.3",
    "jasmine-expect": "^3.6.0",
    "jsdoc": "^3.4.3",
    "karma": "^1.4.0",
    "karma-benchmark": "^0.7.1",
    "karma-benchmarkjs-reporter": "^1.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-matchers": "^3.2.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-webpack": "^2.0.1",
    "npm-run-all": "^4.0.1",
    "uglify-js": "git://github.com/mishoo/UglifyJS2.git#harmony",
    "webpack": "^2.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LokiJS-Forge/LokiJS2.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Various authors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LokiJS-Forge/LokiJS2/issues"
  }
}
