{
	"opts": {
		"readme": "./OVERVIEW.md",
		"destination": "./jsdoc",
		"tutorials": "./tutorials"
	},
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": [
			"jsdoc",
			"closure"
		]
	},
	"source": {
		"include": [
			"src/",
			"src/inverted_index/"
		],
		"includePattern": ".+\\.js$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": [
		"plugins/markdown"
	],
	"templates": {
		"cleverlinks": false,
		"monospaceLinks": false
	}
}
